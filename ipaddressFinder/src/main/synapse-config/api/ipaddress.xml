<?xml version="1.0" encoding="UTF-8"?>
<api context="/ip" name="ipaddress" xmlns="http://ws.apache.org/ns/synapse">
    <resource methods="GET">
        <inSequence>
            <send>
                <endpoint>
                    <address uri="https://postman-echo.com/ip"/>
                </endpoint>
            </send>
        </inSequence>
        <outSequence>
            <send/>
        </outSequence>
        <faultSequence/>
    </resource>
</api>
